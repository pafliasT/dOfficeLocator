<mvc:View xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="com.demo.ZDemo_MasterDetail.controller.master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Locations">
				<List id="orders" items="{/locations}" mode="SingleSelectMaster" selectionChange="setMapLocation">
					<items>
						<StandardListItem title="{locName}" description="{locDesc}" type="Active"/>
					</items>
				</List>
			</Page>
		</pages>
	</App>
</mvc:View>